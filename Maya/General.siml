      <Response>Please specify a product name. Example includes Syn Virtual Assistant, Syn Bot, Syn Engine, Syn Web Assistant or Syn Chatbot Studio.</Response>
      <Model>
        <Pattern>[product]</Pattern>
        <Response>
          <Goto>how can I update <Match /></Goto>
        </Response>
      </Model>

    </Model>

    <Model>
      <Pattern>YOU USE THE SAME CHAT DATA AS SVA DOES</Pattern>
      <Response>Syn Virtual Assistant has my twin sister called Madonna.</Response>
    </Model>

